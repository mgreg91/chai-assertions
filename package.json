{
  "name": "assertions",
  "version": "1.0.0",
  "type": "module",
  "description": "A simple chai assertion excercise",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./ --fix",
    "wdio": "wdio run ./wdio.conf.js",
    "prepare": "husky",
    "test:login": "wdio run ./wdio.conf.js --suite login",
    "test:members": "wdio run ./wdio.conf.js --suite members",
    "test:profile": "wdio run ./wdio.conf.js --suite profile"
  },
  "keywords": [
    "wdio",
    "chai"
  ],
  "author": "Gergő Mátyus",
  "license": "ISC",
  "devDependencies": {
    "@wdio/cli": "^8.32.3",
    "@wdio/local-runner": "^8.32.3",
    "@wdio/mocha-framework": "^8.32.3",
    "@wdio/spec-reporter": "^8.32.2",
    "chai": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "prettier": "3.2.5",
    "prettier-eslint": "^16.3.0",
    "wdio-wait-for": "^3.0.11",
    "husky": "^9.0.11"
  }
}
